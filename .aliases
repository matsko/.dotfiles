function wip {
  MSG=$1
  if [ -z $MSG ]; then
    MSG='feat(core): wip'
  fi
  git add .
  git commit -am $MSG
}

function woo {
  MSG='feat(ivy): wip'
  SUFFIX=$1
  if [ $SUFFIX ]; then
    MSG="${MSG}: ${SUFFIX}"
  fi
  git add .
  git commit -am $MSG
}

